    # c "Lin!"
    c "林!"

# game/scripts/lin_events.rpy:7130
translate schinese lincatchbigfish_f4dea37e:

    # l "Phhphh! Egh! Eeehhhh...!"
    l "噗!呃!呃......!"

# game/scripts/lin_events.rpy:7132
