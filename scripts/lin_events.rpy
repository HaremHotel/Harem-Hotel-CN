    iksa "{i}操!{/i}你在这里不安全!我们完事后会去找你的!"

# game/scripts/lin_events.rpy:5274
translate schinese lin_syltin_e81e42f4:

    # iksa "Run, Lin! RUN!!" with ssshake
    iksa "快跑,林!{b}快跑!!{/b}" with ssshake

# game/scripts/lin_events.rpy:5275
