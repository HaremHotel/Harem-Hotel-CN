    # f "{i}That's her! Sh-She followed us!{/i}"
    f "{i}是她!她-她跟着我们!{/i}"

# game/scripts/clone_events.rpy:1573
translate schinese twin_stalk_f13269dd:

    # em "{i}Oh shit, you're right...{/i}"
    em "{i}哦操,你说得对......{/i}"

# game/scripts/clone_events.rpy:1574
translate schinese twin_stalk_968c90c9:

    # f "{i}She doesn't seem too bothered about us knowing she's watching us.{/i}"
    f "{i}她似乎不太担心我们知道她在监视我们.{/i}"

# game/scripts/clone_events.rpy:1575
translate schinese twin_stalk_64bab4b3:

    # em "{i}Probably because she has a weapon.{/i}"
    em "{i}可能是因为她身上有武器.{/i}"

# game/scripts/clone_events.rpy:1576
translate schinese twin_stalk_1de18d26:

    # c "{i}I'm going to go talk to her.{/i}"
    c "{i}我要去和她谈谈.{/i}"

# game/scripts/clone_events.rpy:1577
translate schinese twin_stalk_9f77247d:

    # em "{i}Are you stupid!?{/i}"
    em "{i}你是傻子吗!?{/i}"

# game/scripts/clone_events.rpy:1579
