    # angel "{b}How dare you!{/b}" with ssshake
    angel "{b}你怎么敢!{/b}" with ssshake

# game/scripts/autumn_events.rpy:1903
translate schinese aut_orient_34dad7a3:

    # au "You're {u}not{/u} an angel!"
    au "你{u}不是{/u}天使!"

# game/scripts/autumn_events.rpy:1905
translate schinese aut_orient_a85db032:

    # angel "..."
    angel "......"

# game/scripts/autumn_events.rpy:1906
translate schinese aut_orient_4dbc4b23:

    # au "...?"
    au "......?"

